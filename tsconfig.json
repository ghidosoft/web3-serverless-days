{
    "compilerOptions": {
        "outDir": "./dist/",
        "removeComments": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "lib": ["dom", "es2015"],
        "importHelpers": true
    }
}
